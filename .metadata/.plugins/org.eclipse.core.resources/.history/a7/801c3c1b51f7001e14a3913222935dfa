package exercicios;
import java.util.Scanner;
public class exercicio24{
	public static void main (String [] args) {
		Scanner input = new Scanner(System.in);
		
		double distanciakm, tempom, velocidade;
		
		System.out.println("Digite a dist√¢ncia em km: ");
		distanciakm = input.nextDouble();
		System.out.println("Digite o tempo em horas: ");
		tempom = input.nextDouble();
		
		velocidade = (distanciakm * 1000) / (tempom * 60);
		
		System.out.println("Velocidade: " + velocidade);
		
		
		input.close();
	}
}