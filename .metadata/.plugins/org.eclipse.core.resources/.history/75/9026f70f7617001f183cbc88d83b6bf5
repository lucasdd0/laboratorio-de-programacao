package checkpoint3;

import java.util.Scanner;

public class exercicio04 {
	public static void main(String[] args) {
		Scanner in = new Scanner (System.in);
		
		int n, i, result = 0;
		
		System.out.println("Digite um nÃºmero: ");
		n = in.nextInt();
		
		for(i = n; i >= 0; i--) {
			result *= i;
			System.out.println(result);
		}
		
		
		
		
		in.close();
	}

}
