package exercicios;

import java.util.Scanner;

public class exercicio13 {
	public static void main(String[] args) {
		Scanner input = new Scanner (System.in);
		
		int i, j, menor;
		int v [] = new int [3];
		
		for (i = 0; i < 3; i++)
		{
			System.out.println("Digite o " + (i + 1) + "Â° valor: ");
			v[i] = input.nextInt();
		}
		
		for (i = 0; i < (3 - 1); i++)
		{
			menor = v[i]
			for (j = i + 1; j < 3; j++)
			{
				if(v[i] > v [j])
				{
					menor = v[j];
				}
			}
		}
		
		

		
		
		input.close();
		
		
		
		
		
		
		
		input.close();
	}
}
