package exercicios;
import java.util.Scanner;
public class exercicio09 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		double l1, l2, l3;
		
		System.out.println("Digite o valor de um lado: ");
		l1 = input.nextDouble();
		System.out.println("Digite o valor de um lado: ");
		l2 = input.nextDouble();
		System.out.println("Digite o valor de um lado: ");
		l3 = input.nextDouble();
		
		if (l1<l2+l3)
		{
			if(l2<l1+l3)
			{
				if (l3<l2+l1) {
					System.out.println("Os lados do seu triangulo sÃ£o" + l1 + ", " + l2 + ", " + l3);
				}
			}
		}
		
		
		
		
		
		
		input.close();
	}

}
