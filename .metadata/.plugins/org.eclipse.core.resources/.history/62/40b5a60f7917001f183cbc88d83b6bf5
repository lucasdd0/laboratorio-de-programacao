package checkpoint3;

import java.util.Scanner;

public class exercicio05 {
	public static void main(String[] args) {
		Scanner in = new Scanner (System.in);
		
		int n;
		
		System.out.println("Digite um nÃºmero: ");
		n = in.nextInt();
		
		int i = 1;
		
		while(n >= 0) {
			n = n * i;
		}
		
		
	}
}
