package checkpoint3;

import java.util.Scanner;

public class exercicio06 {
	public static void main(String[] args) {
		Scanner in = new Scanner (System.in);
		
		int n, i = 1, cont = i - 1;
		
		System.out.println("Digite o total de termos: ");
		n = in.nextInt();
		
		while(i <= n) {
			i = i + cont;
			System.out.println(i);
		}
		
		in.close();
	}

}
