package exercicios;
import java.util.Scanner;
public class exercicio23{
	public static void main (String [] args) {
		Scanner input = new Scanner(System.in);
		
		double a, b, c, nulos, brancos;
		
		System.out.println("Candidado A: ");
		a = input.nextDouble();
		System.out.println("Candidado B: ");
		b = input.nextDouble();
		System.out.println("Candidado C: ");
		c = input.nextDouble();
		System.out.println("Votos em branco: ");
		brancos = input.nextDouble();		
		System.out.println("Votos nulos: ");
		nulos = input.nextDouble();	
		
		
		
		
		input.close();
	}
}