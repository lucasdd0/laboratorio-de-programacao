package checkpoint3;

import java.util.Scanner;

public class exercicio02 {
	public static void main(String[] args) {
		Scanner in = new Scanner (System.in);
		
		int n, i, multi;
		
		System.out.println("Digite um n√∫mero: ");
		n = in.nextInt();
		
		for(i = 0; i <= 10; i++) {
			multi = n * i;
			System.out.println(multi);
		}
		
		
		
		
		
		in.close();
	}

}
