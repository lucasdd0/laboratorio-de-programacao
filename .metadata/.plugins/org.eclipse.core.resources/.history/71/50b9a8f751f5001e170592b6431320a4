package exercicios;
import java.util.Scanner;
public class exercicio29{
	public static void main(String[] args) {
		Scanner input = new Scanner (System.in);
		
		double y= 0;
		
	

		y = Math.pow(((Math.pow(Math.sqrt(2), Math.sqrt(3)) * Math.pow(Math.sqrt(2), Math.sqrt(3)) + Math.pow(Math.sqrt(2), Math.sqrt(12))) / (Math.pow(Math.sqrt(6), Math.sqrt(2)) + Math.pow(Math.sqrt(2), Math.sqrt(3) + Math.sqrt(2)))), 		1 / (Math.sqrt(3) - Math.sqrt(2)));		
		
		
		
		System.out.println("Seu resultado Ã©: " + y);
		
	}
}